{
  "name": "spotify-example-webpack",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build:development": "webpack  --config webpack.dev.config.js --bail",
    "build:production": "webpack --config webpack.prod.config.js --bail",
    "build:homolog": "webpack --config webpack.homolog.config.js --bail",
    "start": "webpack-dev-server --config webpack.dev.config.js --bail",
    "test": "jest",
    "test:nowatch": "jest --watchAll=false",
    "test:watch": "jest --watchAll=true",
    "test:collectcoverage": "jest --collectCoverage",
    "test:coverage": "jest --coverage",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "@storybook/addon-actions": "^5.3.18",
    "@storybook/addon-links": "^5.3.18",
    "@storybook/addons": "^5.3.18",
    "@storybook/react": "^5.3.18",
    "@svgr/webpack": "^5.3.1",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.3.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-dynamic-import-node": "^2.3.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "cd": "^0.3.3",
    "css-loader": "^3.5.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.2.0",
    "husky": "^4.2.5",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^25.3.0",
    "jest-transform-css": "^2.0.0",
    "jest-transform-stub": "^2.0.0",
    "lint-staged": "^10.1.3",
    "mini-css-extract-plugin": "^0.9.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "redux-devtools-extension": "^2.13.8",
    "redux-test-utils": "^0.3.0",
    "style-loader": "^1.1.3",
    "terser": "^4.6.11",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-dotenv-plugin": "^2.1.0"
  },
  "dependencies": {
    "@storybook/addon-knobs": "^5.3.18",
    "@storybook/addon-storysource": "^5.3.18",
    "apisauce": "^1.1.1",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.5",
    "redux-saga": "^1.1.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json}": [
      "eslint --fix",
      "prettier --config .prettierrc --write"
    ]
  },
  "setupFiles": [
    "<rootDir>/src/setupTests.js"
  ],
  "jest": {
    "verbose": true,
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.js"
    ],
    "watchPathIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "modulePaths": [
      "<rootDir>",
      "<rootDir>/src"
    ],
    "transform": {
      "^.+\\.(js|jsx)$": "babel-jest",
      "\\.(css|less|scss|sass)$": "identity-obj-proxy"
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|scss)$": "identity-obj-proxy"
    },
    "globals": {
      "js-jest": {
        "diagnostics": false
      },
      "babel-jest": {
        "diagnostics": false
      }
    },
    "collectCoverage": false,
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/*index.{js,jsx}",
      "!**/node_modules/**",
      "!**/coverage/**",
      "!**/build/**",
      "!**/public/**",
      "!**/src/assets/**"
    ]
  }
}
