{
  "presets": [
    "@babel/preset-react",
    [
      "@babel/preset-env",
      {
        "targets": {
          "node": "current"
        }
      }
    ]
  ],
  "plugins": [
    "@babel/plugin-syntax-dynamic-import",
    "@babel/plugin-proposal-class-properties",
    "babel-plugin-dynamic-import-node"
  ],
  "env": {
    "test": {
      "plugins": ["dynamic-import-node","@babel/plugin-transform-runtime"]
    }
  }
}
